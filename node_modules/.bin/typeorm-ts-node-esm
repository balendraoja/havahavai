#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/airport-api/node_modules/.pnpm/typeorm@0.3.20_sqlite3@5.1.7/node_modules/typeorm/cli-ts-node-esm.js/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/airport-api/node_modules/.pnpm/typeorm@0.3.20_sqlite3@5.1.7/node_modules/typeorm/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/airport-api/node_modules/.pnpm/typeorm@0.3.20_sqlite3@5.1.7/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/airport-api/node_modules/.pnpm/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/airport-api/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/node_modules:/home/nxtwave/nodejs/sessions/node_modules:/home/nxtwave/nodejs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/airport-api/node_modules/typeorm/cli-ts-node-esm.js/node_modules:/home/nxtwave/nodejs/sessions/Introduction-to-Node-JS/airport-api/node_modules/typeorm/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
else 
  exec node  "$basedir/../typeorm/cli-ts-node-esm.js" "$@"
fi
